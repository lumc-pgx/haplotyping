from pipe_helper import PipeHelper

class HaplotypingHelper(PipeHelper):
    @property
    def outputs(self):
        return expand("haplotyping/matches/{barcodes}.matches.json", barcodes=self.barcode_ids) + \
               expand("haplotyping/tables/{barcodes}.matches.txt", barcodes=self.barcode_ids) + \
               expand("haplotyping/haplotypes/{barcodes}.haplotype.txt", barcodes=self.barcode_ids)
